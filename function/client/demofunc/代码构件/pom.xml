								"configs":{}
							},"Dependencies" : {
							"vjs.framework.extension.util.math" : null
							},
							"BundleName":"DemoFunc"
						}</VJS-Package>
						<MatchCode>ClientFunctions.DemoFunc</MatchCode>
						<MatchVersion>1</MatchVersion>
						<MinMatchVersion>1</MinMatchVersion>
					</instructions>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
